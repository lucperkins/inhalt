{
  "name": "inhalt",
  "version": "0.1.0-SNAPSHOT",
  "description": "Content management system",
  "type": "module",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/lucperkins/inhalt",
  "keywords": [
    "cms",
    "content",
    "static-site",
    "jamstack"
  ],
  "scripts": {
