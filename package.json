    "typescript": "^4.5.5",
    "vitest": "^0.3.4"
  },
  "dependencies": {
    "fast-glob": "^3.2.11",
    "rehype-sanitize": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-stringify": "^10.0.2",
    "unified": "^10.1.1"
